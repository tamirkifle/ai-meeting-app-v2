{
    "manifest_version": 3,
    "name": "Meeting Audio Transcription",
    "version": "1.0",
    "permissions": ["tabCapture", "activeTab", "storage"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  